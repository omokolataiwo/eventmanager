{
  "name": "event-management",
  "version": "1.0.0",
  "description":
    "An Event Manager, for listing your event center and booking center for your events.",
  "main": "index.js",
  "scripts": {
    "start:dev":
      "cross-env NODE_ENV=development webpack-dev-server --open webpack.config.js",
    "production":
      "cross-env rimraf dist && webpack --config webpack.prod.config.js --progress --profile --colors",
    "build":
      "cross-env NODE_ENV=production webpack --config webpack.config.js -p",
    "lint": "eslint ./src/**/**.js",
    "test": "jest",
    "nightwatch": "npm run db:reset && NODE_ENV=test nightwatch",
    "db:reset": "cd .. && npm run db:reset-test",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "precommit": "cross-env npm run lint && npm test",
    "prepush": "cross-env npm run lint && npm test"
  },
  "author": "Taiwo Kolawole",
  "license": "MIT",
  "jest": {
    "setupFiles": ["<rootDir>/config/jest/shim.js"],
    "collectCoverageFrom": ["src/**/*.{js,jsx}"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/config/jest/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/config/jest/assetsTransformer.js"
    },
    "testMatch": ["**/?(*.)(spec|test).js?(x)"],
    "transform": {
      "\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chart.js": "^2.7.2",
    "cross-env": "5.1.4",
    "dotenv": "^5.0.1",
    "eslint-config-react": "^1.1.7",
    "faker": "^4.1.0",
    "format-num": "^1.0.0",
    "jquery": "^3.3.1",
    "materialize-css": "^0.100.2",
    "moment": "^2.22.0",
    "prop-types": "^15.6.1",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-hot-loader": "4.0.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "4.2.2",
    "redux": "^3.7.2",
    "redux-persist": "^5.9.1",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^2.3.0",
    "toastr": "^2.1.4",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "validate.js": "^0.12.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "8.2.0",
    "babel-jest": "22.4.1",
    "babel-loader": "7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-boilerplate": "1.1.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "copy-webpack-plugin": "4.5.1",
    "css-hot-loader": "1.3.9",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.6.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^1.1.10",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.15.0-rc.2",
    "jest": "22.1.0",
    "node-sass": "4.8.3",
    "open-browser-webpack-plugin": "0.0.5",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.3.1",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.7",
    "style-loader": "0.20.3",
    "url-loader": "^1.0.0",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.9.6"
  }
}
